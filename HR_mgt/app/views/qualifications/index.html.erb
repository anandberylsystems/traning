<p id="notice"><%= notice %></p>

<h1>Qualifications</h1>

<table>
  <thead>
    <tr>
      <th>Tenth</th>
      <th>Intermediate</th>
      <th>Graduation</th>
      <th>Passing year</th>
      <th>Employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualifications.each do |qualification| %>
      <tr>
        <td><%= qualification.tenth %></td>
        <td><%= qualification.intermediate %></td>
        <td><%= qualification.graduation %></td>
        <td><%= qualification.passing_year %></td>
        <td><%= qualification.employee.name %></td>
        <td><%= link_to 'Show', qualification %></td>
        <td><%= link_to 'Edit', edit_qualification_path(qualification) %></td>
        <td><%= link_to 'Destroy', qualification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qualification ||', new_qualification_path %>


