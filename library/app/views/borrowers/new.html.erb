<%= form_for @borrowers, url: "/create_borrowers" do |f| %>
 <label>Book Id:</label><%= f.text_field :book_id %><br />
 
 <label>Return Date:</label><%= f.text_field :return_date %><br />
 
   <label>Student Id:</label><%= f.text_field :st_id %><br /> 
   <label>Issued By:<%= f.collection_select :issued_id, @staffs, :id, :name %></label>
 <%#= f.collection_select :brand_id, @brands, :id, :name %>
 <%#= f.text_area :body, size: "60x12" %>
 <%= f.submit "Submit" %>
<% end %>